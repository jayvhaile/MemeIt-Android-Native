<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textColor="#313131"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="24k" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textColor="#313131"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="180k" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:textColor="#313131"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="2k" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="51dp"
        tools:text="users" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="51dp"
        tools:text="total memes" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="51dp"
        tools:text="posted today" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="51dp"
        tools:text="crashes" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="#313131"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="131dp"
        tools:text="Version 1.3" />

    <Button
        android:id="@+id/home_template_manager"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#00000000"
        android:text="Template Manager"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="179dp" />

    <View
        android:id="@+id/divider_1"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#28000000"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="525dp" />

    <Button
        android:id="@+id/home_report_manager"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#00000000"
        android:text="Report Manager"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="527dp" />

    <View
        android:id="@+id/divider_2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#28000000"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="587dp" />

    <Button
        android:id="@+id/home_view_feedback"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#00000000"
        android:text="View Feedbacks"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="589dp" />

    <View
        android:id="@+id/divider_3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#28000000"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="649dp" />

    <Button
        android:id="@+id/home_todos"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#00000000"
        android:text="ToDos"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="651dp" />

    <View
        android:id="@+id/divider_4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#28000000"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="711dp" />

</androidx.constraintlayout.widget.ConstraintLayout>